{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe Aktive Datei kompilieren",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/O2",
                "/EHsc",
                "/nologo",
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
                "${file}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Vom Debugger generierte Aufgabe."
        },
        {
            "type": "cppbuild",
            "label": "Build sudoku-count.exe",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/O2",
                "/EHsc",
                "/nologo",
                "/Fe${workspaceFolder}/sudoku-count.exe",
                "${workspaceFolder}/sudoku-2.cpp",
                "${workspaceFolder}/sudoku-3.cpp",
                "${workspaceFolder}/sudoku-4.cpp",
                "${workspaceFolder}/sudoku-count.cpp"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Vom Debugger generierte Aufgabe."
        }
    ],
    "version": "2.0.0"
}